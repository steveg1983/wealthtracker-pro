{
  "rewrites": [
    {
      "source": "/((?!assets/|.*\\.js$|.*\\.css$|.*\\.png$|.*\\.jpg$|.*\\.svg$|.*\\.ico$|.*\\.json$|.*\\.webmanifest$|.*\\.txt$|.*\\.xml$|.*\\.woff$|.*\\.woff2$|.*\\.ttf$|.*\\.eot$).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}