# Client-side (Vite) env vars
# Copy to `.env.local` for local development

# Clerk
VITE_CLERK_PUBLISHABLE_KEY=

# Supabase (client)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Stripe (client)
VITE_STRIPE_PUBLISHABLE_KEY=

# Sentry (optional)
VITE_SENTRY_DSN=
VITE_ENABLE_ERROR_TRACKING=false
VITE_APP_ENV=development

# Feature flags (optional)
# Set to 'false' to disable asset compression during local builds
ENABLE_ASSET_COMPRESSION=true
ANALYZE_BUNDLE=false

# Market Data APIs
# Alpha Vantage - Free tier: 5 API calls per minute, 500 calls per day
# Get your free API key at: https://www.alphavantage.co/support/#api-key
VITE_ALPHA_VANTAGE_API_KEY=demo

