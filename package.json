{
  "name": "wealth-tracker",
  "private": true,
  "version": "1.4.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vite": "vite",
    "build": "vite build",
    "build:check": "npm run typecheck:strict && tsc -b && vite build",
    "build:with-sw-manifest": "vite build && node scripts/generate-sw-manifest.mjs",
    "build:compress": "node scripts/build-with-compression.js",
    "build:production": "npm run build:compress",
    "optimize:images": "node scripts/optimize-images.js",
    "perf:analyze": "node scripts/run-performance-tests.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "VITEST_SUPABASE_MODE=mock vitest --coverage --config vitest.config.unit.ts",
    "test:run": "vitest run",
    "test:unit": "VITEST_SUPABASE_MODE=mock vitest run --config vitest.config.unit.ts",
    "test:integration": "VITEST_SUPABASE_MODE=real vitest run src/test/integration",
    "test:real": "VITEST_SUPABASE_MODE=real vitest run -c vitest.config.integration.ts",
    "test:smoke": "VITEST_SUPABASE_MODE=mock vitest run src/test/decimal-integration.test.tsx src/store/slices/layoutSlice.test.ts",
    "test:seed": "tsx scripts/seed-test-data.ts",
    "test:docker:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:docker:down": "docker-compose -f docker-compose.test.yml down",
    "test:docker:reset": "npm run test:docker:down && npm run test:docker:up && npm run test:seed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "cypress:component": "cypress open --component",
    "sync:server": "node sync-server.js",
    "dev:sync": "concurrently \"npm run dev\" \"npm run sync:server\"",
    "bench": "vitest bench",
    "bench:run": "vitest bench --run",
    "bundle:check": "node scripts/bundle-size-check.js",
    "bundle:report": "node scripts/bundle-size-check.js --report",
    "bundle:summary": "node scripts/print-bundle-summary.mjs",
    "deps:usage": "node scripts/deps-usage.mjs",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "perf:test": "npm run build && npm run bundle:check && npm run lighthouse",
    "test:analyze": "node scripts/test-coverage-analysis.js",
    "test:generate": "node scripts/generate-test-templates.js high-priority",
    "test:generate:single": "node scripts/generate-test-templates.js single",
    "deploy": "./deploy.sh",
    "deploy:prod": "./deploy.sh prod",
    "vercel:login": "vercel login",
    "vercel:link": "vercel link",
    "vercel:env": "vercel env pull",
    "clean:artifacts": "bash -c 'rm -rf dist coverage playwright-report'",
    "typecheck:strict": "NODE_OPTIONS=\"--max-old-space-size=4096\" tsc -p tsconfig.strict.json --noEmit",
    "verify:ci": "npm run typecheck:strict && npm run lint && npm run test:ci",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.42.1",
