    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/jspdf": "^1.3.3",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "ag-grid-community": "^34.1.2",
    "ag-grid-react": "^34.1.2",
